import numpy
my_array = ([])
l = input().split()
st = int(l[0])
stro = int(l[1])
for i in range(stro):

    r = list(map(int, input().split()))
    my_array.append(r)
std = (numpy.std(my_array, axis = None))
print (numpy.mean(my_array, axis = 1))
print (numpy.var(my_array, axis = 0))
print (numpy.around(std, 11))
